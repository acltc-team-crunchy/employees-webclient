<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>SSN</th>
    <th>Birthdate</th>
    <th>Email</th>
  </tr>
  <% @employees.each do |employee| %>
  <tr>
    <td><%= employee["first_name"] %></td>
    <td><%= employee["last_name"] %></td>
    <td><%= employee["ssn"] %></td>
    <td><%= employee["birthday"] %></td>
    <td><%= employee["email"] %></td>
  </tr>
  <% end %>
</table>