<table>
  <tr>
    <th>Full Name</th>
    <th>SSN</th>
    <th>Birthdate</th>
    <th>Email</th>
  </tr>
  <% @employees.each do |employee| %>
  <tr>
    <td><%= employee.full_name %></td>
    <td><%= employee.ssn %></td>
    <td><%= employee.birthday %></td>
    <td><%= employee.email %></td>
  </tr>
  <% end %>
</table>